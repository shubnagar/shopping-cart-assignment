{
  "name": "shopping-cart",
  "version": "1.0.0",
  "description": "This is a shopping cart coding exercise",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode production",
    "dev:build:client": "webpack --config client/webpack.client.ts --watch",
    "dev:build:server": "webpack --config client/webpack.server.ts --watch",
    "dev:server": "nodemon --watch client/dist --exec \"node client/dist/main.js\"",
    "start:client": "npm-run-all --parallel dev:server dev:build:client dev:build:server",
    "start:server": "canned -p 5000 ./server"
  },
  "keywords": [
    "Shopping",
    "Cart",
    "Products",
    "Ecommerce"
  ],
  "author": "Shubham Nagar",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "babel-loader": "^9.1.2",
    "canned": "^0.3.12",
    "css-loader": "^6.7.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
